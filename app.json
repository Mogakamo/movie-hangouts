{
  "expo": {
    "name": "movie_hangouts",
    "slug": "movie_hangouts",
    "version": "1.0.0",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./src/client/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/client/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/client/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mogaka_dev.movie_hangouts"
    },
    "web": {
      "favicon": "./src/client/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "56689825-cb5f-423d-9b59-c8bc75ddf873"
      }
    }
  }
}
